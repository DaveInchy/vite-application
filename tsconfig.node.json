{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2017",
    "composite": true,
    "skipLibCheck": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "types": [
      "@types/react-router-dom",
      "@types/react-dom",
      "@types/react",
      "@types/node"
    ]
  },
  "exclude": [
    "public",
    "api"
  ],
  "include": [
    "vite.config.ts"
  ]
}
